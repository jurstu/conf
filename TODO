# Download maps if necessary
MAPS="dataset/urban_maps.zip"
if [ ! -z "$MAPS" ]; then
  wget https://zenodo.org/record/1211730/files/urban_maps.zip?download=1 -O $MAPS
  unzip $MAPS -d dataset/
fi
